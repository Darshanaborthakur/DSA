#include<iostream>
using namespace std;
int main()
{
	int n,i;
	cout<<"How many numbers do you have\n";
	cin>>n;
	int *arr=new int(n);
	for(int i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	int largest=arr[0];
	for(int i=1;i<n;i++)
	{
		if(arr[i]>largest)
		{
			largest=arr[i];
		}
		cout<<"largest number:"<<largest;
	}
	return 0;
}
