#include<iostream>
using namespace std;
int main()
{
	int n,i,j,c;
	cout<<"How many elements do you want\n";
	cin>>n;
	int *arr=new int(n);
	cout<<"Enter the numbers\n";
	for(int i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	cout<<"The unique elements are"<<arr[i];
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<n;j++)
		{
			if(arr[i]=arr[j])
			{
				c++;
			}
			
		}
		if(c==1)
		{
			cout<<arr[i]<<" ,";
			c=0;
		}
	
	
	}
	return 0;
}
