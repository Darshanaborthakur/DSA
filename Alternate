#include<iostream>
using namespace std;
int main()
{
	int n,i;
	cout<<"Enter the number of elements\n";
	cin>>n;
	int arr[n];
	cout<<"enter the elements\n";
	for(int i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	cout<<"The alternate array is:"<<endl;
	for(int i=0;i<n;i+=2)
	{
		cout<<arr[i]<<" ,";
	}
	cout<<"The alternate array is"<<arr[i];
	return 0;
}
